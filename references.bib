@article{neco.1995.7.6.1289,
author = {Bengio, Yoshua and LeCun, Yann and Nohl, Craig and Burges, Chris},
title = {LeRec: A NN/HMM Hybrid for On-Line Handwriting Recognition},
journal = {Neural Computation},
